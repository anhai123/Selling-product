{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/home"
    },

    {
      "src": "/shopping/auth/(.*)",
      "dest": "/"
    },
    {
      "src": "/categories/(.*)",
      "dest": "/"
    },

    {
      "src": "/api/comments/(.*)",
      "dest": "/"
    }
    ,

    {
      "src": "/payment/(.*)",
      "dest": "/"
    },

    {
      "src": "/products/(.*)",
      "dest": "/"
    }
    ,

    {
      "src": "/api/shopping/user/(.*)",
      "dest": "/"
    }
    
  ]
  
}